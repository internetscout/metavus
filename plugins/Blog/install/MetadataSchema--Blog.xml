<?xml version="1.0" encoding="UTF-8" ?>
<MetadataSchema>
    <StandardFieldMapping>
        <StandardName>Title</StandardName>
        <Name>Title</Name>
    </StandardFieldMapping>
    <StandardFieldMapping>
        <StandardName>Description</StandardName>
        <Name>Summary</Name>
    </StandardFieldMapping>
    <StandardFieldMapping>
        <StandardName>Screenshot</StandardName>
        <Name>Image</Name>
    </StandardFieldMapping>
    <MetadataField>
        <Name>Blog Name</Name>
        <Type>MDFTYPE_OPTION</Type>
        <Description>The blog name which this entry is from.</Description>
        <Owner>Blog</Owner>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
        <Optional>0</Optional>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>3</SearchWeight>
    </MetadataField>
    <MetadataField>
        <Name>Title</Name>
        <Type>MDFTYPE_TEXT</Type>
        <Description>The title of a blog entry.</Description>
        <Owner>Blog</Owner>
        <Optional>0</Optional>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>1</IncludeInRecommender>
        <SearchWeight>5</SearchWeight>
        <RecommenderWeight>5</RecommenderWeight>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Body</Name>
        <Type>MDFTYPE_PARAGRAPH</Type>
        <Description>The text of the blog entry. Inserting an extra blank line after a paragraph will cause the teaser to end at that point if the text up to that point is less than the maximum teaser length.</Description>
        <Owner>Blog</Owner>
        <Optional>0</Optional>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>1</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <RecommenderWeight>1</RecommenderWeight>
        <AllowHTML>1</AllowHTML>
        <UseWysiwygEditor>1</UseWysiwygEditor>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Summary</Name>
        <Type>MDFTYPE_PARAGRAPH</Type>
        <Description>Autogenerated summary of blog content. This field cannot be directly edited.</Description>
        <Enabled>1</Enabled>
        <Optional>0</Optional>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>0</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <RecommenderWeight>0</RecommenderWeight>
        <Editable>0</Editable>
    </MetadataField>
    <MetadataField>
        <Name>Image</Name>
        <Type>MDFTYPE_IMAGE</Type>
        <Description>An image associated with the blog entry.</Description>
        <Owner>Blog</Owner>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>1</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <RecommenderWeight>1</RecommenderWeight>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
        <AllowMultiple>1</AllowMultiple>
    </MetadataField>
    <MetadataField>
        <Name>Categories</Name>
        <Type>MDFTYPE_CONTROLLEDNAME</Type>
        <Description>Terms or a nonhierarchical string of terms relating to the content of the blog entry.</Description>
        <Owner>Blog</Owner>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>1</IncludeInRecommender>
        <SearchWeight>3</SearchWeight>
        <RecommenderWeight>3</RecommenderWeight>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Notifications Sent</Name>
        <Type>MDFTYPE_FLAG</Type>
        <Description>Flag to determine whether notifications have been sent for the blog entry.</Description>
        <Owner>Blog</Owner>
        <Editable>0</Editable>
        <DefaultValue>0</DefaultValue>
        <FlagOnLabel>Yes</FlagOnLabel>
        <FlagOffLabel>No</FlagOffLabel>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <CopyOnResourceDuplication>0</CopyOnResourceDuplication>
        <SearchWeight>1</SearchWeight>
        <ViewingPrivileges>
            <AddPrivilege>PRIV_SYSADMIN</AddPrivilege>
        </ViewingPrivileges>
        <AuthoringPrivileges>
            <AddPrivilege>PRIV_SYSADMIN</AddPrivilege>
        </AuthoringPrivileges>
        <EditingPrivileges>
            <AddPrivilege>PRIV_SYSADMIN</AddPrivilege>
        </EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Date of Creation</Name>
        <Type>MDFTYPE_TIMESTAMP</Type>
        <Description>The date the blog entry was authored.</Description>
        <Owner>Blog</Owner>
        <Editable>0</Editable>
        <Optional>0</Optional>
        <UpdateMethod>OnRecordCreate</UpdateMethod>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <AuthoringPrivileges>1</AuthoringPrivileges>
        <EditingPrivileges>1</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Date of Modification</Name>
        <Type>MDFTYPE_TIMESTAMP</Type>
        <Description>The date the blog entry was last edited.</Description>
        <Owner>Blog</Owner>
        <Editable>0</Editable>
        <Optional>0</Optional>
        <UpdateMethod>OnRecordChange</UpdateMethod>
        <TriggersAutoUpdates>0</TriggersAutoUpdates>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <AuthoringPrivileges>1</AuthoringPrivileges>
        <EditingPrivileges>1</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Date of Publication</Name>
        <Type>MDFTYPE_TIMESTAMP</Type>
        <Description>The date on which the blog entry was or should be published. The value may be in the future to delay publication.</Description>
        <Owner>Blog</Owner>
        <Optional>0</Optional>
        <UpdateMethod>Button</UpdateMethod>
        <TriggersAutoUpdates>0</TriggersAutoUpdates>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <RecommenderWeight>0</RecommenderWeight>
        <AuthoringPrivileges>2</AuthoringPrivileges>
        <EditingPrivileges>2</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Author</Name>
        <Type>MDFTYPE_USER</Type>
        <Description>The author of a blog entry.</Description>
        <Owner>Blog</Owner>
        <Editable>0</Editable>
        <Optional>0</Optional>
        <UpdateMethod>OnRecordCreate</UpdateMethod>
        <TriggersAutoUpdates>0</TriggersAutoUpdates>
        <IncludeInKeywordSearch>1</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>3</SearchWeight>
        <UserPrivilegeRestrictions>2</UserPrivilegeRestrictions>
        <AuthoringPrivileges>1</AuthoringPrivileges>
        <EditingPrivileges>1</EditingPrivileges>
    </MetadataField>
    <MetadataField>
        <Name>Editor</Name>
        <Type>MDFTYPE_USER</Type>
        <Description>The last editor of a blog entry.</Description>
        <Owner>Blog</Owner>
        <Editable>0</Editable>
        <Optional>0</Optional>
        <UpdateMethod>OnRecordEdit</UpdateMethod>
        <TriggersAutoUpdates>0</TriggersAutoUpdates>
        <UserPrivilegeRestrictions>2</UserPrivilegeRestrictions>
        <IncludeInKeywordSearch>0</IncludeInKeywordSearch>
        <IncludeInAdvancedSearch>1</IncludeInAdvancedSearch>
        <IncludeInRecommender>0</IncludeInRecommender>
        <SearchWeight>1</SearchWeight>
        <AuthoringPrivileges>1</AuthoringPrivileges>
        <EditingPrivileges>1</EditingPrivileges>
    </MetadataField>
</MetadataSchema>
