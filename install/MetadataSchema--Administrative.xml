		<Owner>MetavusCore</Owner>
		<Editable>0</Editable>
                <TriggersAutoUpdates>0</TriggersAutoUpdates>
                <UpdateMethod>OnRecordRelease</UpdateMethod>
                <CopyOnResourceDuplication>0</CopyOnResourceDuplication>
	</MetadataField>
	<MetadataField>
		<Name>Date Last Modified</Name>
		<Type>TIMESTAMP</Type>
